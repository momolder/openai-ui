{
  "name": "open-ai-svelte",
  "version": "0.0.7",
  "private": true,
  "scripts": {
    "dev": "vavite-loader vite dev",
    "build": "vite build --mode example",
    "prenode": "cp .env ./build/.env",
    "node": "node --require dotenv/config --import ./appinsights.js ./build",
    "docker:build": "docker build --pull --rm -f \"Dockerfile\" -t openaiui:latest \".\" --progress=plain",
    "predocker:run": "npm run docker:build",
    "docker:run": "docker run --rm -d -p 80:80/tcp openaiui:latest --openai",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --fix",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "playwright test"
  },
  "devDependencies": {
    "@jill64/universal-sanitizer": "^1.2.9",
    "@playwright/test": "^1.43.1",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.7.1",
    "@vavite/node-loader": "^4.1.1",
    "@zerodevx/svelte-toast": "^0.9.5",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.38.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "shiki": "^1.3.0",
    "svelte": "^4.2.15",
    "svelte-check": "^3.7.0",
    "svelte-markdown": "^0.4.1",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@azure/openai": "1.0.0-beta.12",
    "@azure/storage-blob": "^12.17.0",
    "@microsoft/applicationinsights-web": "^3.2.0",
    "applicationinsights": "^3.0.1",
    "jspdf": "^2.5.1",
    "tiktoken": "^1.0.14"
  },
  "type": "module"
}
