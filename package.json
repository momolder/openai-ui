{
  "name": "open-ai-svelte",
  "version": "0.0.7",
  "private": true,
  "scripts": {
    "dev": "vavite-loader vite dev",
    "build": "vite build --mode example",
    "prenode": "cp .env ./build/.env",
    "node": "node --require dotenv/config --import ./appinsights.js ./build",
    "docker:build": "docker build --pull --rm -f \"Dockerfile\" -t openaiui:latest \".\" --progress=plain",
    "predocker:run": "npm run docker:build",
    "docker:run": "docker run --rm -d -p 80:80/tcp openaiui:latest --openai",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --fix",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "playwright test"
  },
  "devDependencies": {
    "@jill64/universal-sanitizer": "^1.2.10",
    "@playwright/test": "^1.43.1",
    "@sveltejs/adapter-node": "^2.1.2",
    "@sveltejs/kit": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vavite/node-loader": "^4.1.1",
    "@zerodevx/svelte-toast": "^0.9.5",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.38.0",
    "playwright": "^1.43.1",
    "postcss": "^8.4.38",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "shiki": "^1.4.0",
    "svelte": "^4.2.15",
    "svelte-check": "^3.7.0",
    "svelte-markdown": "^0.4.1",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@azure/openai": "^1.0.0-beta.12",
    "@azure/search-documents": "^12.0.0",
    "@azure/storage-blob": "^12.17.0",
    "@langchain/azure-openai": "^0.0.7",
    "@langchain/community": "^0.0.51",
    "@langchain/core": "0.1.61",
    "@langchain/openai": "^0.0.28",
    "@microsoft/applicationinsights-web": "^3.2.0",
    "applicationinsights": "^2.9.5",
    "cheerio": "^1.0.0-rc.12",
    "langchain": "^0.1.36",
    "pdf-parse": "^1.1.1"
  },
  "overrides": {
    "@langchain/core": "0.1.61"
  },
  "type": "module"
}